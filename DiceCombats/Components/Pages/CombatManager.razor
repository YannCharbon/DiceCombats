@page "/combatmanager/{combatGuidStr}"

<CombatEditor CombatGuid="@combatGuidStr" EditionMode="@EditonMode"></CombatEditor>

@code {
    [Parameter]
    public string combatGuidStr { get; set; } = "new";

    [SupplyParameterFromQuery] // This binds the query string parameter
    public bool EditonMode { get; set; } = false;
}
